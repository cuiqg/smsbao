{
    "version": 2,
    "name": "smsbao",
    "alias": ["smsbao"],
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.3.0"
        }
    },
    "routes": [
        { "src": "/(.*)", "dest": "/api/index.php" },
        { "src": "/agent", "dest": "/api/agent.php" }
    ],
    "github": {
        "enabled": true,
        "autoAlias": false
    }
}
